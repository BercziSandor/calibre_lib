        <dc:title>Harcikutya</dc:title>
        <dc:creator opf:file-as="Moorcock, Michael" opf:role="aut">Michael Moorcock</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (6.10.0) [https://calibre-ebook.com]</dc:contributor>
