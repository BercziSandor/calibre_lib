        <dc:title>Eszem, tehát vagyok</dc:title>
        <dc:creator opf:file-as="Spencer, Bud &amp; Luca, Lorenzo de" opf:role="aut">Bud Spencer</dc:creator>
        <dc:creator opf:file-as="Spencer, Bud &amp; Luca, Lorenzo de" opf:role="aut">Lorenzo de Luca</dc:creator>
