        <dc:title>Hercules munkái</dc:title>
        <dc:creator opf:file-as="Christie, Agatha &amp; Tábori, Zoltán" opf:role="aut">Agatha Christie</dc:creator>
        <dc:creator opf:file-as="Christie, Agatha &amp; Tábori, Zoltán" opf:role="aut">Zoltán Tábori</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (7.1.0) [https://calibre-ebook.com]</dc:contributor>
