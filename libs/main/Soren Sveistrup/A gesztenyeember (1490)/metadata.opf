        <dc:title>A gesztenyeember</dc:title>
        <dc:creator opf:file-as="Sveistrup, Søren" opf:role="aut">Søren Sveistrup</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (6.10.0) [https://calibre-ebook.com]</dc:contributor>
