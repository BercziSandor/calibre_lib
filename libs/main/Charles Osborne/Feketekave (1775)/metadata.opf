        <dc:title>Feketekávé</dc:title>
        <dc:creator opf:file-as="Osborne, Charles &amp; Prekop, Gabriella" opf:role="aut">Charles Osborne</dc:creator>
        <dc:creator opf:file-as="Osborne, Charles &amp; Prekop, Gabriella" opf:role="aut">Gabriella Prekop</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (7.1.0) [https://calibre-ebook.com]</dc:contributor>
