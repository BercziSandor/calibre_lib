        <dc:title>Hát-társ</dc:title>
        <dc:creator opf:file-as="Caledonian, Peter S." opf:role="aut">Peter S. Caledonian</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (6.10.0) [https://calibre-ebook.com]</dc:contributor>
