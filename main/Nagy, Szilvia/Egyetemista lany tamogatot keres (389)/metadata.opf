        <dc:title>Egyetemista lány támogatót keres</dc:title>
        <dc:creator opf:file-as="Szilvia, Nagy &amp; Eszter, Szabó Anna" opf:role="aut">Nagy, Szilvia</dc:creator>
        <dc:creator opf:file-as="Szilvia, Nagy &amp; Eszter, Szabó Anna" opf:role="aut">Szabó, Anna Eszter</dc:creator>
