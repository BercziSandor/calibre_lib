        <dc:title>A svéd, akinek nyoma veszett</dc:title>
        <dc:creator opf:file-as="Sjöwall, Maj &amp; Wahlöö, Per" opf:role="aut">Maj Sjöwall</dc:creator>
        <dc:creator opf:file-as="Sjöwall, Maj &amp; Wahlöö, Per" opf:role="aut">Per Wahlöö</dc:creator>
