        <dc:title>Már lángolt, amikor ráfeküdtem</dc:title>
        <dc:creator opf:file-as="Fulghum, Robert" opf:role="aut">Robert Fulghum</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (6.10.0) [https://calibre-ebook.com]</dc:contributor>
