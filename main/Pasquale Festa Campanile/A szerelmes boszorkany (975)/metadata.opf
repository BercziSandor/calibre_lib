        <dc:title>A szerelmes boszorkány</dc:title>
        <dc:creator opf:file-as="Campanile, Pasquale Festa" opf:role="aut">Pasquale Festa Campanile</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (6.10.0) [https://calibre-ebook.com]</dc:contributor>
